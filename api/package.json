{
  "name": "cognivo",
  "version": "1.0.0",
  "description": "AI-powered study platform with note generation, quiz creation, and Q&A system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:coverage": "jest --coverage --coverageDirectory=coverage",
    "test:performance": "jest --testPathPattern=performance",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts --ext .ts",
    "lint:fix": "eslint src/**/*.ts --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "clean": "rimraf dist coverage",
    "prebuild": "npm run clean",
    "prestart": "npm run build",
    "docker:build": "docker build -t cognivo .",
    "docker:run": "docker run -p 3000:3000 cognivo",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:migrate": "node dist/scripts/migrate.js",
    "db:seed": "node dist/scripts/seed.js",
    "docs:generate": "typedoc src --out docs",
    "docs:serve": "serve docs",
    "metrics:export": "node dist/scripts/export-metrics.js",
    "health:check": "curl -f http://localhost:3000/health || exit 1",
    "setup": "node setup.js",
    "test:api": "node test-api-key.js"
  },
  "keywords": [
    "ai",
    "education",
    "study",
    "notes",
    "quiz",
    "translation"
  ],
  "author": "Cognivo Team",
  "license": "MIT",
  "dependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.20.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "sharp": "^0.34.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tesseract.js": "^5.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.8.0",
    "@types/pdf-parse": "^1.1.1",
    "@types/pg": "^8.10.7",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "serve": "^14.2.1",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.2",
    "typescript": "^5.2.2"
  }
}
